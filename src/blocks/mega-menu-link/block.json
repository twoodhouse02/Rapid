{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rapid/mega-menu-link",
  "version": "0.1.0",
  "title": "Mega Menu Link",
  "category": "rapid-theme",
  "icon": "smiley",
  "parent": ["rapid/mega-menu"],
  "description": "A detailed link block for the mega menu.",
  "attributes": {
    "label": {
      "type": "string",
      "default": "Mega Menu Link"
    },
    "displayDescription": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "string",
      "default": "This is a mega menu link."
    },
    "url": {
      "type": "string",
      "default": ""
    },
    "opensInNewTab": {
      "type": "boolean",
      "default": false
    },
    "displayIcon": {
      "type": "boolean",
      "default": true
    },
    "iconName": {
      "type": "string",
      "default": "cube"
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "mega-menu-link",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
