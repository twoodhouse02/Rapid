{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rapid/ordered-list",
  "version": "0.1.0",
  "title": "Ordered List",
  "category": "rapid-theme",
  "icon": "editor-ol",
  "description": "A dynamic block for displaying a customizable ordered list.",
  "example": {
    "attributes": {
      "items": [
        "This is item 1",
        "This is item 2",
        "This is item 3"
      ]
    }
  },
  "supports": {
    "html": false
  },
  "attributes": {
    "items": {
      "type": "array",
      "default": []
    }
  },
  "textdomain": "ordered-list",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}