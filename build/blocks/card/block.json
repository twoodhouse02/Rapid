{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rapid/card",
  "version": "0.1.0",
  "title": "Card",
  "category": "rapid-theme",
  "icon": "cover-image",
  "description": "Example block scaffolded with Create Block tool.",
  "attributes": {
    "variant": {
      "type": "string",
      "default": "standard"
    },
    "displayHeader": {
      "type": "boolean",
      "default": false
    },
    "displayHeaderIcon": {
      "type": "boolean",
      "default": true
    },
    "headerText": {
      "type": "string",
      "default": "Header text here..."
    },
    "headerIcon": {
      "type": "string",
      "default": "megaphone-outline"
    },
    "displayImage": {
      "type": "boolean",
      "default": true
    },
    "imageURL": {
      "type": "string",
      "default": "https://images.pexels.com/photos/2382325/pexels-photo-2382325.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2"
    },
    "displayEyebrow": {
      "type": "boolean",
      "default": true
    },
    "eyebrowText": {
      "type": "string",
      "default": "Eyebrow text here..."
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "card",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}