{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rapid/navigation-link",
  "parent": [
    "rapid/navigation-menu",
    "rapid/navigation-link"
  ],
  "version": "0.1.0",
  "title": "Navigation Link",
  "category": "rapid-theme",
  "description": "A link block specific to the the Rapid theme Navigation Menu block.",
  "attributes": {
    "label": {
      "type": "string"
    },
    "opensInNewTab": {
      "type": "boolean",
      "default": false
    },
    "url": {
      "type": "string"
    },
    "isParent": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "navigation-link",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}