{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rapid/navigation-menu",
  "version": "0.1.0",
  "title": "Navigation Menu",
  "category": "rapid-theme",
  "icon": "smiley",
  "description": "A navigation block with custom responsive breakpoints",
  "attributes": {
    "breakpoint": {
      "type": "number",
      "default": 768
    },
    "animated": {
      "type": "boolean",
      "default": true
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "navigation-menu",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}